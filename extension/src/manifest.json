{
  "name": "__MSG_name__",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "32": "icons/icon-32.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["content_script.js"],
      "run_at": "document_idle",
      "all_frames": false,
      "matches": ["https://stackoverflow.com/*"]
    }
  ],
  "web_accessible_resources": ["content_script.css", "execute.js"],
  "commands": {
    "copy-all": {
      "suggested_key": {
        "default": "Alt+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Copy all code from page"
    }
  },
  "background": {
    "js": "background.js",
    "persistent": false
  },
  "permissions": ["storage", "activeTab", "tabs"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  }
}
